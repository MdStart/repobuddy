# .coveragerc to control coverage.py
[run]
source = repobuddy
branch = True

[report]
precision = 2
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    def __repr__

    if _sys.version_info

    def add(Error|Failure|Success|Skip|ExpectedFailure|UnexpectedSuccess)

    def show_results
